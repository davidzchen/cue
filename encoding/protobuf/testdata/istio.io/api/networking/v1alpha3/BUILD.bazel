load("@io_bazel_rules_go//go:def.bzl", "go_library")
load("@io_bazel_rules_go//proto:def.bzl", "go_proto_library")

proto_library(
    name = "v1alpha3_proto",
    srcs = ["gateway.proto"],
    visibility = ["//visibility:public"],
    deps = ["//cue:cue_proto"],
)

go_proto_library(
    name = "v1alpha3_go_proto",
    importpath = "istio.io/api/networking/v1alpha3",
    proto = ":v1alpha3_proto",
    visibility = ["//visibility:public"],
    deps = ["//cue:go_default_library"],
)

go_library(
    name = "go_default_library",
    embed = [":v1alpha3_go_proto"],
    importpath = "istio.io/api/networking/v1alpha3",
    visibility = ["//visibility:public"],
)
